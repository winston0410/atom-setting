{
	"*": {
		"atom-beautify": {
			"css": {},
			"jade": {
				"beautify_on_save": true
			},
			"js": {},
			"json": {},
			"jsx": {
				"default_beautifier": "JS Beautify"
			},
			"markdown": {
				"beautify_on_save": true,
				"default_beautifier": "Tidy Markdown"
			},
			"nunjucks": {},
			"php": {
				"beautify_on_save": true
			},
			"typescript": {
				"beautify_on_save": true
			},
			"xml": {
				"beautify_on_save": true
			},
			"yaml": {}
		},
		"atom-minify": {
			"autoHideNotifications": "Info, Success, Error",
			"autoHidePanel": "Success, Error",
			"cssMinifier": "clean-css",
			"minifyOnSave": true,
			"showSavingInfo": false
		},
		"atom-uglify-es": {
			"onSave": true,
			"uglifyToFile": true
		},
		"core": {
			"closeDeletedFileTabs": true,
			"customFileTypes": {
				"source.js": [
					"cjs",
					"mjs"
				],
				"text.html.basic": [
					"svelte"
				]
			},
			"disabledPackages": [
				"git-diff",
				"language-css",
				"markdown-preview",
				"linter",
				"language-babel"
			],
			"packagesWithKeymapsDisabled": [
				"atom-minify"
			],
			"telemetryConsent": "no"
		},
		"docblockr": {
			"extra_tags": [
				"@author Hugo Sum",
				"@lastUpdateDate {{date}}"
			]
		},
		"editor": {
			"fontSize": 25,
			"softWrap": true
		},
		"exception-reporting": {
			"userId": "370b85d8-9633-4dbd-821d-ef00a59c9761"
		},
		"ftp-remote-edit": {
			"config": "",
			"password": "7b287db6442cd6202865"
		},
		"git-diff": {
			"showIconsInEditorGutter": true
		},
		"git-diff-plus": {
			"showIconsInEditorGutter": true
		},
		"linter": {
			"lintOnChange": false
		},
		"linter-eslint": {
			"autofix": {
				"fixOnSave": true
			},
			"global": {}
		},
		"linter-ui-default": {
			"panelHeight": 300
		},
		"minimap": {
			"absoluteMode": true,
			"charHeight": 12,
			"charWidth": 6
		},
		"platformio-ide-terminal": {
			"style": {
				"theme": "linux"
			}
		},
		"react": {
			"enabledForAllJavascriptFiles": true
		},
		"standard-formatter": {
			"honorPackageConfig": false
		},
		"sync-settings": {
			"autoCheckForUpdatedBackup": "no",
			"hiddenSettings": {},
			"useOtherLocation": true
		},
		"sync-settings-git-location": {
			"commitMessagePrompt": true,
			"gitUrl": "https://github.com/winston0410/atom-setting.git"
		},
		"tree-view": {},
		"welcome": {
			"showOnStartup": false
		},
		"zentabs": {}
	},
	".blade.html.php.text": {
		"editor": {
			"commentEnd": " --}}",
			"commentStart": "{{-- "
		}
	},
	".coffee.jsx.source": {
		"editor": {
			"softWrap": true
		}
	},
	".js.jsx.source": {
		"editor": {
			"softWrap": true
		}
	}
}